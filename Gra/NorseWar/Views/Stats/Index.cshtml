@model IEnumerable<NorseWar.Models.Stats>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Account.Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Str)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Agi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Int)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Luk)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Account.Login)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Str)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Agi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Int)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Luk)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StatsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.StatsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StatsID })
        </td>
    </tr>
}

</table>
